{
  "name": "custom-framing-app-2",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "engines": {
    "node": "v12.14.1"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build",
    "seed": "node scripts/seedDB.js",
    "seedUsers": "node scripts/userSeed.js",
    "seedCart": "node scripts/cartSeed.js"
  }, 
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evwhite2/CustomFramingApp2.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/evwhite2/CustomFramingApp2/issues"
  },
  "homepage": "https://github.com/evwhite2/CustomFramingApp2#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "mongoose": "^5.9.2",
    "node-mongo-seeds": "^3.1.0",
    "nodemon": "^2.0.2"
  },
  "devDependencies": {
    "express": "^4.17.1",
    "if-env": "^1.0.4"
  }
}
